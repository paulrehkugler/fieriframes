{
  "name": "fieriframes",
  "version": "1.0.0",
  "description": "Guy Fieri Frames",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shahkashani/fieriframes.git"
  },
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "node logo.js && ./node_modules/.bin/connect",
    "post": "node logo.js && ./node_modules/.bin/post --tag='Guy Fieri'",
    "stills": "node logo.js && ./node_modules/.bin/stills --faces=10 --num=5 --captions=100",
    "gifs": "node logo.js && ./node_modules/.bin/gifs --num=1 --width=540 --captions=100",
    "train": "node logo.js && ./node_modules/.bin/train --name=fieri"
  },
  "author": "Shah Kashani",
  "license": "ISC",
  "dependencies": {
    "stills": "git+https://github.com/shahkashani/stills.git"
  }
}
